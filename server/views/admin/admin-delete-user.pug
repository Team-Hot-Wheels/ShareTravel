include admin-main.pug
  h2 Choose a user 
  if (data)
   table.table.table-responsive
    thead
     tr 
      th Username:
      th Action:
    tbody  
        each user in data
         if (user.username === 'admin')
         else
            tr
            td
             strong= user.username
            td 
             form(method="post" action="/admin/admin-delete-user") 
              input(type="hidden" value=user.username name="username")
              button(type="submit" class="btn btn-default btn-danger") Delete
                